---
import { IconUpRightFromSquare } from "@/icons/up-right-from-square";
import styles from "./styles.module.css";

type Props = {
  href: string;
};

const { href } = Astro.props;
---

<a {href} class={styles.container} target="_blank" rel="noreferrer"
  ><slot /><IconUpRightFromSquare class={styles.icon} /></a
>
