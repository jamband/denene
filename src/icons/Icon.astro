---
import type { HTMLAttributes } from "astro/types";

export type Props = {
  viewBox: HTMLAttributes<"svg">["viewBox"];
  style: HTMLAttributes<"svg">["style"];
};
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  fill="currentColor"
  role="img"
  aria-hidden={true}
  viewBox={Astro.props.viewBox}
  style={Astro.props.style}
  ><slot />
</svg>
