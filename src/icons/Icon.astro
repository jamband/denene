---
import type { HTMLAttributes } from "astro/types";

type Props = {
  viewBox: HTMLAttributes<"svg">["viewBox"];
  style: HTMLAttributes<"svg">["style"];
};

const { props } = Astro;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  fill="currentColor"
  role="img"
  aria-hidden={true}
  viewBox={props.viewBox}
  style={props.style}
  ><slot />
</svg>
